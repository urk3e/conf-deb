{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
        "os": "#CC6666",
        "closer": "p:os",
        "pink": "#80A1BD",
        "lavender": "#B294BA",
        "blue":  "#F0C574"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
	{
	  "foreground": "p:pink",
	  "style": "plain",
	  "template": "[",
	  "type": "os"
	},
        {
          "foreground": "p:os",
          "style": "plain",
          "template": " {{.Icon}} ",
          "type": "os",
	  "size": 13.0
        },
        {
          "foreground": "p:blue",
          "style": "plain",
          "template": "{{ .UserName }}@{{ .HostName }} ",
          "type": "session"
        },
        {
          "foreground": "p:pink",
          "properties": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": "{{ .Path }} ]",
          "type": "path"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}

